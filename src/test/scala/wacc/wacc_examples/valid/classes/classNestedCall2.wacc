# method within nested class accesses

# Output:
# 12
# 6

# Program:


begin 
    class Car {
        Wheel wheel 
    }

    class Wheel {
        Tyre tyre 
    }

    class Tyre {
        int diameter(int x) is
            return x
        end
    }

    Car ford = new Car {new Wheel {new Tyre {}}} ;
    Wheel michelin = new Wheel {new Tyre {}} ; 
    int d = call ford.wheel.tyre.diameter(12) ;
    println d ;
    ford.wheel = michelin ;
    int e = call ford.wheel.tyre.diameter(6) ;
    println e
end