# method within nested class accesses

# Output:
# 12
# 6

# Program:


begin 
    class Car {
        Wheel wheel 
    }

    class Wheel {
        Tyre tyre 
    }

    class Tyre {
        int diameter(int x) is
            return x
        end
    }

    Tyre t1 = new Tyre {} ;
    Car ford = new Car {new Wheel {t1}} ;
    Tyre t2 = new Tyre {} ;
    Wheel michelin = new Wheel {t2} ; 
    int d = call ford.wheel.tyre.diameter(12) ;
    println d ;
    ford.wheel = michelin ;
    int e = call ford.wheel.tyre.diameter(6) ;
    println e
end