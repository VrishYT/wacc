# method within nested class accesses

# Output:

# Exit:
# 5

# Program:


begin 
    class Car {
        Wheel wheel 
    }

    class Wheel {
        Tyre tyre 
    }

    class Tyre {
        int diameter(int x) is
            return x
        end
    }

    Tyre t = new Tyre {} ;
    Car ford = new Car {new Wheel {t}} ;
    int d = call ford.wheel.tyre.diameter(5) ;
    exit d
end